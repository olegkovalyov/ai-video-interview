  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:all": "npm run test && npm run test:integration && npm run test:e2e -- --forceExit",
    "test:integration": "jest --config jest.integration.config.js",
    "test:integration:watch": "jest --config jest.integration.config.js --watch",
    "test:integration:cov": "jest --config jest.integration.config.js --coverage",
    "test:e2e": "jest --config jest.e2e.config.js",
    "test:e2e:watch": "jest --config jest.e2e.config.js --watch",
    "test:e2e:cov": "jest --config jest.e2e.config.js --coverage",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm -- migration:generate -d src/infrastructure/persistence/typeorm.config.ts",
    "migration:run": "npm run typeorm -- migration:run -d src/infrastructure/persistence/typeorm.config.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/infrastructure/persistence/typeorm.config.ts",
    "migration:show": "npm run typeorm -- migration:show -d src/infrastructure/persistence/typeorm.config.ts",
    "migration:test:run": "DATABASE_NAME=ai_video_interview_user_test npm run typeorm -- migration:run -d src/infrastructure/persistence/typeorm.config.ts",
    "migration:test:revert": "DATABASE_NAME=ai_video_interview_user_test npm run typeorm -- migration:revert -d src/infrastructure/persistence/typeorm.config.ts",
    "migration:test:show": "DATABASE_NAME=ai_video_interview_user_test npm run typeorm -- migration:show -d src/infrastructure/persistence/typeorm.config.ts"
  },
